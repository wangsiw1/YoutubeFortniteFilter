{
    "name": "Youtube Fartnite Filter",
    "version": "0.4",
    "manifest_version": 2,

    "description": "Filter out all Fortnite video on the youtube you don't want to see!",
    "icons": {
        "400": "icon.png"
    },
    
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": ["fartfilter.js"]
        }
    ],

    "browser_action": {
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs", "webNavigation"
    ]

}